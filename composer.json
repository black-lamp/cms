{
    "name": "black-lamp/cms",
    "description": "Black Lamp CMS project",
    "keywords": ["yii2", "cms"],
    "homepage": "https://github.com/black-lamp/yii2-bl-cms",
    "type": "project",
    "license": "GNU General Public License",
    "support": {
        "issues": "https://github.com/black-lamp/yii2-bl-cms/issues?state=open",
        "source": "https://github.com/black-lamp/yii2-bl-cms"
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": ">=2.0.6",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "black-lamp/yii2-articles": "*"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
